<div class="standard-body fixed-body-2-2">
    <div id="main" class="">
        <!-- HEADER
    ================================================== -->
        <header class="header-type-2">
            <div class="background-header"></div>
            <div class="slider-header">
                <!-- Top of pages -->
                <div id="top" class="full-width">
                    <div class="background-top"></div>
                    <div class="background">
                        <div class="shadow"></div>
                        <div class="pattern">
                            <div class="top-bar">
                                <div class="container">
                                    <!-- Links -->
                                    <ul class="menu">
                                        <li><a href="{{ account }}">{{ text_account }}</a></li>
                                        <li><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}">{{ text_wishlist }}</a></li>
                                        <li><a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}">{{ text_shopping_cart }}</a></li>
                                        <li><a href="{{ checkout }}" title="{{ text_checkout }}">{{ text_checkout }}</a></li>
                                    </ul>
                                    <!-- Currency -->
                                    {{ currency }}
                                </div>
                            </div>

                            <div class="container">
                                <div class="row">
                                    <!-- Header Left -->
                                    <div class="col-sm-4" id="header-left">
                                        <!-- Search -->
                                        {{ search }}
                                    </div>

                                    <!-- Header Center -->
                                    <div class="col-sm-4" id="header-center">
                                        <!-- Logo -->
                                        <div class="logo">
                                            {% if logo %}
                                                <a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}"></a>
                                            {% else %}
                                                <h1><a href="{{ home }}">{{ name }}</a></h1>
                                            {% endif %}
                                        </div>
                                    </div>

                                    <!-- Header Right -->
                                    <div class="col-sm-4" id="header-right">

                                        <a href="{{ account }}" class="my-account"><i class="fa fa-user"></i></a>

                                        <!-- Cart block -->
                                        {{ cart }}
                                    </div>
                                </div>
                            </div>

                            {{ menu }}

                        </div>
                    </div>
                </div>
            </div>
        </header>