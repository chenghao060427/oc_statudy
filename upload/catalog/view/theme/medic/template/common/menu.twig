<div class="standard-body">
    <div id="main" class="">
        <header class="header-type-8">
            <div class="background-header"></div>
            <div class="slider-header">
                <!-- Top of pages -->
                <div id="top" class="full-width">
                    <div class="background-top"></div>
                    <div class="background">
                        <div class="shadow"></div>
                        <div class="pattern">
                            <div class="container">
                                <div class="row">
                                    <!-- Header Left -->
                                    <div class="col-sm-4" id="header-left">
                                        <!-- Logo -->
                                        <div class="logo">
                                            {% if logo %}
                                                <a href="{{ home }}">
                                                    <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}">
                                                </a>
                                            {% else %}
                                                <h1><a href="{{ home }}">{{ name }}</a></h1>
                                            {% endif %}
                                        </div>
                                    </div>

                                    <!-- Header Right -->
                                    <div class="col-sm-8" id="header-right">
                                        <div class="medic-phone hidden-md hidden-sm">
                                            <img src="catalog/view/theme/medic/image/icon-phone.png" alt="Phone">
                                            <div class="heading">Do you have<br>a question?</div>
                                            <p>{{ telephone }}</p>
                                        </div>

                                        <!-- Search -->
                                        {{ search }}
                                        <a href="{{ account }}"
                                           class="my-account"><i class="fa fa-user"></i>
                                        </a>
                                        <!-- Cart block -->
                                        {{ cart }}
                                    </div>
                                </div>
                            </div>

                            <div class="megamenu-background">
                                <div class="">
                                    <div class="overflow-megamenu container">
                                        <div class="row mega-menu-modules">
                                            <div class="col-md-3">
                                                <div id="megamenu_11973796"
                                                     class="container-megamenu container vertical mobile-disabled">
                                                    <div id="menuHeading">
                                                        <div class="megamenuToogle-wrapper">
                                                            <div class="megamenuToogle-pattern">
                                                                <div class="container">
                                                                    {{ catagories_name }}
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="megamenu-wrapper">
                                                        <div class="megamenu-pattern">
                                                            <div class="container">
                                                                <ul class="megamenu shift-left">
                                                                    {% for category in categories %}
                                                                        {% if category.children %}
                                                                            <li class="with-sub-menu hover">
                                                                                <p class="close-menu"></p>
                                                                                <p class="open-menu"></p>
                                                                                <a href="{{ category.href }}" class="clearfix">
                                                                                    <span>
                                                                                        <strong>{{ category.name }}</strong>
                                                                                    </span>
                                                                                </a>
                                                                                <div class="sub-menu " style="width:190px">
                                                                                    <div class="content"><p class="arrow"></p>
                                                                                        <div class="row">
                                                                                            <div class="col-sm-12  mobile-enabled">
                                                                                                <div class="row">
                                                                                                    <div class="col-sm-12 hover-menu">
                                                                                                        <div class="menu">
                                                                                                            <ul>
                                                                                                                {% for child in category.children %}
                                                                                                                <li>
                                                                                                                    <a href="{{ child.href }}"  class="main-menu ">{{ child.name }}</a>
                                                                                                                </li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </li>
                                                                        {% else %}
                                                                            <li class=""><p class="close-menu"></p>
                                                                                <p class="open-menu mobile-disabled"></p>
                                                                                <a href="{{ category.href }}" class="clearfix">
                                                                                    <span>
                                                                                        <strong>
                                                                                            {{ category.name }}
                                                                                            {% if catagory.is_new %}
                                                                                            <span class="megamenu-label" style="background: #0a191b;color:  #ffffff;">
                                                                                                <span style="background: #0a191b;border-color: #0a191b">
                                                                                                </span>
                                                                                                NEW
                                                                                            </span>
                                                                                            {% endif %}
                                                                                            {% if catagory.is_hot %}
                                                                                            {% endif %}
                                                                                        </strong>
                                                                                    </span>
                                                                                </a>
                                                                            </li>
                                                                        {% endif %}
                                                                    {% endfor %}
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

